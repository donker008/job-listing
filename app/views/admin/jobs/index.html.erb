<% content_for(:sidebar) do %>
  <%= render :partial => "sidebar" %>
<% end %>

<div class="container-fluid">
<% if current_user && current_user.is_admin? && !isUserDashBoard %>
<div class="group">
  <%= link_to("发布岗位", new_admin_job_path, class:"btn btn-primary pull-right btn_new_job")%>
</div>
<% end %>
<table class="table table-boldered table-striped custab">
<thead>

  <th class="tab_cell_job">岗位</th>
  <!-- <th>description</th> -->
  <th>薪资</th>
  <th>联系方式</th>
  <th>发布日期</th>
  <% if !isUserDashBoard %>
  <th colspan="4">编辑</th>
  <% end %>
</thead>
<tbody>
  <%= render :partial => "jobitem", :collection => @jobs , :as => :job  %>
</tbody>
</table>
</div>
